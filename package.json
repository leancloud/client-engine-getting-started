{
  "name": "client-engine-nodejs-getting-started",
  "version": "0.1.0",
  "private": true,
  "description": "A sample LeanCloud Client Engine app",
  "main": "build/src/index.js",
  "scripts": {
    "lint": "tslint src/*",
    "lint:fix": "tslint src/* --fix",
    "build": "tsc --build tsconfig.json",
    "docs": "typedoc src --out docs",
    "start": "node build/src/index.js",
    "postinstall": "npm run build && npm run docs",
    "benchmark": "npm run build && memory-usage build/benchmark/empty-room.js"
  },
  "keywords": [
    "leancloud",
    "client-engine",
    "master-client"
  ],
  "license": "MIT",
  "dependencies": {
    "@leancloud/play": "^0.13.15",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "debug": "^3.1.0",
    "express": "^4.16.3",
    "express-basic-auth": "^1.1.5",
    "lodash": "^4.17.10",
    "nanoid": "^1.2.1",
    "p-queue": "^3.0.0",
    "rxjs": "^6.3.2"
  },
  "engines": {
    "node": "10.x"
  },
  "devDependencies": {
    "@types/cors": "^2.8.4",
    "@types/debug": "0.0.30",
    "@types/express": "^4.16.0",
    "@types/lodash": "^4.14.116",
    "@types/nanoid": "^1.0.0",
    "@types/p-queue": "^2.3.1",
    "memory-usage": "^1.2.1",
    "tslint": "^5.11.0",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.1"
  }
}
